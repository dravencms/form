{
  "name": "dravencms/form",
  "type": "dravencms-package",
  "description": "Form module for dravencms",
  "keywords": [
    "dravencms",
    "form"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/structure": "@dev",
    "dravencms/recaptcha": "@dev",
    "dravencms/templated-email": "@dev"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "form": "Dravencms\\Form\\DI\\FormExtension"
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Form\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Form\\Script\\PreUninstall"
      }
    }
  }
}